$anujq('.gw-select').each(function(){showMegaMenuChild($anujq(this),0);});}
function slideUpID(id){$anujq(id).stop(true,true).slideUp('fast');}
function slideIn(id){if($anujq(id).is(':visible')){if(id=="#gw-mobile-menu-wrap #menu0"){$anujq('#gw-mobile-menu-wrap #menuword').html('Menu');}
if(id=="#gw-mobile-menu-wrap #mobilesearch"){$anujq('#gw-mobile-menu-wrap #searchword').html('');if($anujq('.srch-sel-site').css('display')!='none'){$anujq('.srch-sel-site').hide();$anujq('#srch-sel-arrow1').attr('src','//style.anu.edu.au/_anu/4/images/buttons/arrow-down-black.png');$anujq('#srch-sel-arrow').attr('src','//style.anu.edu.au/_anu/4/images/buttons/arrow-down-black.png');}}
$anujq("#gw-menuslidecover").animate({'height':'0'},{duration:'fast',complete:function(){$anujq(id).hide();}});$anujq('.gw-menuslide').hide();}
else{if(id=="#gw-mobile-menu-wrap #menu0"){if($anujq('#gw-mobile-menu-wrap #mobilesearch').is(':visible')){$anujq('#gw-mobile-menu-wrap #searchword').html('');$anujq('#gw-mobile-menu-wrap #mobilesearch').hide();}
if($anujq('.srch-sel-site').css('display')!='none'){$anujq('.srch-sel-site').hide();$anujq('#srch-sel-arrow1').attr('src','//style.anu.edu.au/_anu/4/images/buttons/arrow-down-black.png');$anujq('#srch-sel-arrow').attr('src','//style.anu.edu.au/_anu/4/images/buttons/arrow-down-black.png');}
$anujq('.gw-menuslide').show();$anujq('#gw-mobile-menu-wrap #menuword').html('Close');hopen=$anujq('#gw-menuslidecover .gw-menuslide').last().css('height');}
else if(id=="#gw-mobile-menu-wrap #mobilesearch"){if($anujq('#gw-mobile-menu-wrap #menu0').is(':visible')){$anujq('#gw-mobile-menu-wrap #menuword').html('Menu');$anujq('#gw-mobile-menu-wrap #menu0').hide();}
$anujq('.gw-menuslide').hide();$anujq('#gw-mobile-menu-wrap #searchword').html('Close');hopen=$anujq(id).css('height');}
$anujq(id).show();$anujq("#gw-menuslidecover").animate({'height':hopen},{duration:'fast',easing:'swing',complete:function(){}});}}
function hideMegaMenu(){if($anujq('.gw-mega-wrap').is(':visible')){$anujq('#gw-nav a').removeClass('gw-mega-select');$anujq('#tabs-nav a').removeClass('gw-mega-select');$anujq('#gw-mega-under').slideUp('fast');$anujq('.gw-mega-wrap').slideUp('fast');}}
function showMegaMenu(id){$anujq('#gw-nav a').removeClass('gw-mega-select');if($anujq('#gw-mega-'+id).is(':visible')){$anujq('#gw-mega-under').slideUp('fast');$anujq('.gw-mega-wrap').slideUp('fast');}else{if($anujq('.gw-mega-wrap').is(':visible')){$anujq('.gw-mega-wrap').fadeOut('fast');$anujq('#gw-mega-'+id).fadeIn('fast');$anujq('#gw-nav-wrap #gw-nav a:eq('+(id-1)+')').addClass('gw-mega-select');$anujq('#tabs-wrap #gw-nav #gw-mega-tab-'+(id)+'').addClass('gw-mega-select');}
else{$anujq('#gw-nav-wrap #gw-nav a:eq('+(id-1)+')').addClass('gw-mega-select');$anujq('#tabs-wrap #gw-nav #gw-mega-tab-'+(id)+'').addClass('gw-mega-select');$anujq('#gw-mega-under').slideDown('fast');$anujq('#gw-mega-'+id).stop(true,true).slideDown('fast',function(){});}}}
function showMegaMenuChild(id,anim){thisul=$anujq(id).parent().children('ul');if(thisul.length==0){return;}
newul=$anujq(id).parent().children('ul').clone().addClass('gw-mega-mobile-list').show();if($anujq(id).parents('.gw-menuslide').attr('id')=='menu0'){oldheading="home";}else{oldheading=$anujq(id).parents('.gw-mega-mobile-list').children('li:eq(1)').children('a').html();}
newul.prepend("<li><a class='gw-menu-this' href='"+$anujq(id).attr('href')+"'>"+$anujq(id).html()+"</a></li>");newul.prepend("<li><a class='gw-menu-back' href='#' onclick='menuBack($anujq(this)); return false;'>&laquo; Back <span class='gw-menu-back-where'>to "+oldheading+"</span></a></li>");newlast=$anujq('<div class="gw-menuslide gw-menu-current" >');newlast.prepend(newul).css({'top':'0','left':$anujq("#gw-menuslidecover").css('width')});currlast=$anujq('#gw-menuslidecover .gw-menuslide').last();$anujq(currlast).after(newlast);if(anim==1){$anujq(currlast).animate({'left':'-960px'},{duration:400,easing:'swing',complete:function(){},queue:false});$anujq("#gw-menuslidecover").animate({'height':$anujq('.gw-menu-current').css('height')},{duration:'fast',easing:'swing'});$anujq('.gw-menu-current').animate({'left':'0'},400,'swing');}else{$anujq(currlast).css('left','-960px');$anujq('.gw-menu-current').css('left','0');}
$anujq('.gw-menu-current').removeClass('gw-menu-current');}
function menuBack(id){currlast=$anujq('#gw-menuslidecover .gw-menuslide').last();prevul=currlast.prev();prevul.show();currlast.animate({'left':'960px'},{duration:400,easing:'swing',complete:function(){},queue:false});$anujq("#gw-menuslidecover").animate({'height':prevul.css('height')},{duration:'fast',easing:'swing',complete:function(){currlast.remove();}});prevul.animate({'left':'0'},400,'swing');}