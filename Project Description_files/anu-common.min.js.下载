function fireGSAsuggest(query){request=$anujq.ajax({url:"http://find.anu.edu.au/suggest",crossDomain:true,dataType:"jsonp",jsonpCallback:"loaded_gsa_sug",data:{max:"10",site:"public_collection",client:"anu_frontend",format:"os",q:query}});}
function loaded_gsa_sug(data){$anujq(".gsasuggest").html('');if(data[1].length==0){$anujq(".gsasuggest").hide();return;}
if(canShowGSASuggest){$anujq(".gsasuggest").show();}
var newul=document.createElement("ul");for(i=0;i<data[1].length;i++){data[1][i]=data[1][i].replace(/\\x22/g,'&#34;');data[1][i]=data[1][i].replace(/\\x27/g,'&#39;');data[1][i]=data[1][i].replace(/\\x26/g,'&#38;');data[1][i]=data[1][i].replace(/\\x3c/g,'&#60;');data[1][i]=data[1][i].replace(/\\x3e/g,'&#62;');$anujq(newul).append("<li><a href='http://find.anu.edu.au/search?client=anu_frontend&proxystylesheet=anu_frontend&site=default_collection&btnG=Search&filter=0&q="+data[1][i]+"'><strong>"+data[1][i].substr(0,data[0].length)+"</strong>"+data[1][i].substr(data[0].length)+"</a></li>");}
$anujq(".gsasuggest").append(newul);}
